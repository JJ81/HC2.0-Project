{{! common data }}
<meta name="naver-site-verification" content="a153f94a1cbbeecf0056394854faf2192600cbc5" />
<meta name="description" content="텍사스 홀덤, 홀덤, 포커, 포커 전문 영상, 홀덤교육, 홀덤스쿨, 홀덤 콘텐츠, 토너먼트" />
<meta name="keyword" content="텍사스 홀덤, 홀덤, 포커, 포커 전문 영상, 홀덤교육, 홀덤스쿨, 홀덤 콘텐츠, 토너먼트" />
<meta name="author" content="홀덤클럽티비" />
<meta http-equiv="x-dns-prefetch-control" content="on" />
<link rel="canonical" href="http://holdemclub.tv/" />
<link rel='stylesheet' href='/static/stylesheets/styles.min.css?v={{version}}' media="screen,handheld" />
{{! DNS Prefetch setting for CDN }}
<link rel="dns-prefetch" href="http://static.holdemclub.tv/" />
<link rel="dns-prefetch" href="http://media.holdemclub.tv/" />
{{! 페이지뷰가 낮을 경우 prefetch를 꺼버린다. }}
{{! <meta http-equiv="x-dns-prefetch-control" content="off" /> }}
<title>{{title}}</title>
</head>

<body>
{{! Google Tag Manager (noscript) }}
<noscript>
    <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-KT2VT6" height="0" width="0" style="display:none;visibility:hidden"></iframe>
</noscript>
{{! End Google Tag Manager (noscript) }}

    <div id="holdemclub">
        {{> header}}

        {{! start content }}
        <div id="content">
            <div class="inner_content clearfix">
                {{> left_nav}}
                <div class="main_area">
                    <div id="spot">
                        <a href="/" target="_blank">
                            <img src="/static/images/banner_download.jpg" alt="홀덤클럽포 다운로드" width="840" />
                        </a>
                    </div>

                    <div class="content_body">
                        {{{body}}}
                    </div>
                </div>
            </div>
        </div>
        {{! // end content }}

        {{> footer}}
    </div>

    {{#if (isEquals current_path 'INDEX')}}<script src="/static/javascripts/dist/index.js?v={{version}}"></script>{{/if}}
    {{#if (isEquals current_path 'SIGNUP')}}<script src="/static/javascripts/dist/signup.js?v={{version}}"></script>{{/if}}
    {{#if (isEquals current_path 'VIDEOLIST')}}<script src="/static/javascripts/dist/video_list.js?v={{version}}"></script>{{/if}}
    {{#if (isEquals current_path 'VIDEOVIEW')}}
        <script src="/static/javascripts/vendor/videojs.js?v={{version}}"></script>
        <script src="/static/javascripts/vendor/youtube.min.js?v={{version}}"></script>
        <script src="/static/javascripts/dist/video_view.js?v={{version}}"></script>
    {{/if}}

    {{! Google Tag Manager }}
